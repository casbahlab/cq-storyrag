ex:MusicInstrument a rdfs:Class ;
    rdfs:label "Musical Instrument" ;
    rdfs:comment "A physical or digital instrument used to perform music." ;
    rdfs:subClassOf schema:Thing .

ex:MedicalEvent a rdfs:Class ;
    rdfs:label "Medical Event" ;
    rdfs:comment "A medical or health-related incident relevant to a music performance or event." ;
    rdfs:subClassOf schema:Event .

ex:hasChallenge a rdf:Property ;
    rdfs:label "has challenge" ;
    rdfs:comment "Links an event to a challenge it faced, such as logistical or technical issues." ;
    rdfs:domain schema:Event ;
    rdfs:range ex:EventChallenge .

ex:wasBroadcastPreserved a rdf:Property ;
    rdfs:domain schema:BroadcastEvent ;
    rdfs:range xsd:boolean .

ex:charitySingle a rdf:Property ;
    rdfs:label "charity single" ;
    rdfs:comment "Indicates whether a music release is a charity single." ;
    rdfs:domain schema:MusicRecording ;
    rdfs:range xsd:boolean .

ex:isBestSellingIn a rdf:Property ;
    rdfs:label "is best selling in" ;
    rdfs:comment "Links a music release to the country where it was the best seller." ;
    rdfs:domain schema:MusicRecording ;
    rdfs:range schema:Country .

ex:attendance a rdf:Property ;
    rdfs:label "attendance" ;
    rdfs:comment "Number of people attending a live event or performance." ;
    rdfs:domain schema:Event ;
    rdfs:range xsd:integer .

ex:hadBroadcast a rdf:Property ;
    rdfs:domain schema:MusicEvent ;
    rdfs:range schema:BroadcastEvent .

ex:distributedVia a rdf:Property ;
    rdfs:label "distributed via" ;
    rdfs:comment "Describes the medium or method through which a broadcast event was distributed, as a literal string." ;
    rdfs:domain schema:BroadcastEvent ;
    rdfs:range xsd:string .

ex:debutYear a rdf:Property ;
    rdfs:label "debut year" ;
    rdfs:comment "The year in which a musical artist or group first debuted." ;
    rdfs:domain schema:MusicArtist ;
    rdfs:range xsd:gYear .

ex:hasDebutRelease a rdf:Property ;
    rdfs:label "has debut release" ;
    rdfs:comment "Links a music artist to their first release (album or single)." ;
    rdfs:domain schema:MusicArtist ;
    rdfs:range schema:MusicRelease .

ex:performedAt a rdf:Property ;
    rdfs:label "performed at" ;
    rdfs:comment "Indicates that a music artist performed at a specific live event." ;
    rdfs:domain schema:MusicArtist ;
    rdfs:range schema:Event .

ex:facilitatedBy a rdf:Property, owl:ObjectProperty ;
    rdfs:domain schema:BroadcastEvent ;
    rdfs:range schema:Satellite ;
    rdfs:label "facilitated by" ;
    rdfs:comment "Indicates the satellite or broadcast infrastructure that facilitated the transmission of a broadcast event." .

ex:earlyMusicInfluence a rdf:Property ;
    rdfs:label "early music influence" ;
    rdfs:comment "Links a music artist to a music genre that influenced their early style." ;
    rdfs:domain mm:MusicArtist ;
    rdfs:range mm:MusicGenre .

ex:inspiredEvent a rdf:Property ;
    rdfs:label "inspired event" ;
    rdfs:comment "Indicates that an event inspired the creation of another event." ;
    rdfs:domain schema:Event ;
    rdfs:range schema:Event .


ex:numberOfActs a rdf:Property ;
    rdfs:label "number of acts" ;
    rdfs:comment "Number of acts or performances at an event." ;
    rdfs:domain schema:Event ;
    rdfs:range schema:Number .

ex:raisedAmount a rdf:Property ;
    rdfs:label "raised amount" ;
    rdfs:comment "Total amount of money raised by the event." ;
    rdfs:domain schema:Event ;
    rdfs:range schema:MonetaryAmount .

ex:usedBroadcastInfrastructure a rdf:Property ;
    rdfs:label "used broadcast infrastructure" ;
    rdfs:comment "Infrastructure used to broadcast the event." ;
    rdfs:domain schema:BroadcastEvent ;
    rdfs:range schema:Organization .

ex:wasBroadcastBy a rdf:Property ;
    rdfs:label "was broadcast by" ;
    rdfs:comment "The organizations that broadcast the event." ;
    rdfs:domain schema:BroadcastEvent ;
    rdfs:range schema:Organization .

ex:wasBroadcastVia a rdf:Property ;
    rdfs:label "was broadcast via" ;
    rdfs:comment "Mediums through which the event was broadcast (radio, TV, satellite, etc.)." ;
    rdfs:domain schema:BroadcastEvent ;
    rdfs:range schema:Text .

ex:playsInstrument a rdf:Property ;
    rdfs:label "plays instrument" ;
    rdfs:comment "Indicates the musical instrument a person plays." ;
    rdfs:domain schema:Person ;
    rdfs:range ex:MusicInstrument .

