    rdfs:domain ex:CharitableConcert ;
    rdfs:range ex:Cause .

ex:criticalAcclaim a rdf:Property ;
    rdfs:domain ex:Performance ;
    rdfs:range schema:Review .

ex:hasPerformer a rdf:Property ;
    rdfs:label "has performer" ;
    rdfs:comment "Relates a broadcast or music event to the performing artist or group." ;
    rdfs:domain ex:BroadcastMusicEvent ;
    rdfs:range ex:MusicArtist ;
    rdfs:subPropertyOf schema:performer .

ex:hasSetList a rdf:Property ;
    rdfs:domain ex:Performance ;
    rdfs:range ex:PerformanceSegment .

ex:influenced a rdf:Property ;
    rdfs:domain ex:BroadcastMusicEvent .

ex:inspiredBy a rdf:Property ;
    rdfs:domain ex:BroadcastMusicEvent .

ex:occurredDuringSegment a rdf:Property ;
    rdfs:domain ex:MemorableMoment ;
    rdfs:range ex:PerformanceSegment .

ex:performedIn a rdf:Property ;
    rdfs:domain schema:MusicComposition ;
    rdfs:range ex:PerformanceSegment .

ex:performedSong a rdf:Property ;
    rdfs:label "performed song" ;
    rdfs:comment "The song performed during this segment." ;
    rdfs:domain ex:PerformanceSegment ;
    rdfs:range schema:MusicRecording .

ex:performer a rdf:Property ;
    rdfs:label "performer" ;
    rdfs:comment "Links a performance to the artist or group who performed." ;
    rdfs:domain ex:Performance ;
    rdfs:range schema:MusicGroup .

ex:performerIn a rdf:Property ;
    rdfs:label "performerIn" ;
    rdfs:comment "An event in which the music artist performed." ;
    rdfs:domain ex:MusicArtist ;
    rdfs:range schema:Event .

<http://example.org/rdfs:label> a rdf:Property ;
    rdfs:domain ex:MusicGenre ;
    rdfs:range xsd:string .

ex:segmentOf a rdf:Property ;
    rdfs:label "segment of" ;
    rdfs:comment "Links this segment to the larger performance it belongs to." ;
    rdfs:domain ex:PerformanceSegment ;
    rdfs:range ex:Performance .

ex:songPerformed a rdf:Property ;
    rdfs:domain ex:PerformanceSegment ;
    rdfs:range schema:MusicComposition .

schema:description a rdf:Property ;
    rdfs:label "description" ;
    rdfs:comment "A description of the item." ;
    rdfs:domain ex:PerformanceSegment ;
    rdfs:range xsd:string ;
    owl:equivalentProperty dcterms:description ;
    schema:domainIncludes schema:Thing ;
    schema:rangeIncludes schema:Text,
        schema:TextObject .

schema:domainIncludes a rdf:Property ;
    rdfs:label "domainIncludes" ;
    rdfs:comment "Relates a property to a class that is (one of) the type(s) the property is expected to be used on." ;
    schema:domainIncludes schema:Property ;
    schema:isPartOf <https://meta.schema.org> ;
    schema:rangeIncludes schema:Class .

schema:isPartOf a rdf:Property ;
    rdfs:domain schema:BroadcastEvent,
        schema:Event,
        schema:MusicEvent .

schema:location a rdf:Property ;
    rdfs:label "location" ;
    rdfs:comment "The location of, for example, where an event is happening, where an organization is located, or where an action takes place." ;
    rdfs:domain ex:Cause,
        schema:CreativeWork,
        schema:Event,
        schema:MusicEvent ;
    rdfs:range schema:Country ;
    schema:domainIncludes schema:Action,
        schema:Event,
        schema:InteractionCounter,
        schema:Organization ;
    schema:rangeIncludes schema:Place,
        schema:PostalAddress,
        schema:Text,
        schema:VirtualLocation .

schema:position a rdf:Property ;
    rdfs:label "position" ;
    rdfs:comment "The position of an item in a series or sequence of items." ;
    rdfs:domain ex:PerformanceSegment ;
    rdfs:range xsd:integer ;
    schema:domainIncludes schema:CreativeWork,
        schema:ListItem ;
    schema:rangeIncludes schema:Integer,
        schema:Text .

schema:rangeIncludes a rdf:Property ;
    rdfs:label "rangeIncludes" ;
    rdfs:comment "Relates a property to a class that constitutes (one of) the expected type(s) for values of the property." ;
    schema:domainIncludes schema:Property ;
    schema:isPartOf <https://meta.schema.org> ;
    schema:rangeIncludes schema:Class .

schema:sameAs a rdf:Property ;
    rdfs:label "sameAs" ;
    rdfs:comment "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website." ;
    rdfs:domain ex:Cause ;
    rdfs:range schema:URL ;
    schema:domainIncludes schema:Thing ;
    schema:rangeIncludes schema:URL .

