  ex:LiveAid1985 schema:amountRaised ?amount .
}


# CQ-E21 (Exit): Which organization preserved the Wembley broadcast? (RAG-ready)
PREFIX ex: <http://example.org/liveaid/>
PREFIX schema: <https://schema.org/>
PREFIX mm: <https://w3id.org/polifonia/ontology/music-meta/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
SELECT ?broadcast ?broadcastLabel ?broadcaster ?broadcasterLabel ?broadcasterURL ?archived
WHERE {
  ex:WembleyBroadcast a mm:Broadcast ;
                      rdfs:label ?broadcastLabel ;
                      mm:hasBroadcaster ?broadcaster ;
                      schema:archived ?archived .
  ?broadcaster rdfs:label ?broadcasterLabel .
  OPTIONAL { ?broadcaster schema:sameAs ?broadcasterURL }
  OPTIONAL { ?broadcaster rdfs:seeAlso ?broadcasterURL }
  OPTIONAL { ?broadcaster schema:subjectOf ?broadcasterURL }
  BIND(ex:WembleyBroadcast AS ?broadcast)
}
ORDER BY ?broadcasterLabel


# CQ-E22 (Exit): Which organization failed to preserve the Philadelphia broadcast? (RAG-ready)
PREFIX ex: <http://example.org/liveaid/>
PREFIX mm: <https://w3id.org/polifonia/ontology/music-meta/>
PREFIX schema: <https://schema.org/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
SELECT ?broadcast ?broadcastLabel ?broadcaster ?broadcasterLabel
       (GROUP_CONCAT(DISTINCT ?broadcasterURL; SEPARATOR="|") AS ?broadcasterURLs)
       ?archived
WHERE {
  # Philadelphia broadcast node
  ex:PhiladelphiaBroadcast a mm:Broadcast ;
                           rdfs:label ?broadcastLabel ;
                           mm:hasBroadcaster ?broadcaster ;
                           schema:archived ?archived .
  ?broadcaster rdfs:label ?broadcasterLabel .
  OPTIONAL { ?broadcaster schema:sameAs ?broadcasterURL }
  OPTIONAL { ?broadcaster rdfs:seeAlso ?broadcasterURL }
  OPTIONAL { ?broadcaster schema:subjectOf ?broadcasterURL }
  BIND(ex:PhiladelphiaBroadcast AS ?broadcast)
}
GROUP BY ?broadcast ?broadcastLabel ?broadcaster ?broadcasterLabel ?archived
ORDER BY ?broadcasterLabel



# CQ-E24 (Exit): Why is Queen's Live Aid performance considered iconic?
PREFIX ex: <http://example.org/liveaid/>
PREFIX mm: <https://w3id.org/polifonia/ontology/music-meta/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
SELECT ?comment
WHERE {
  ex:QueenLiveAidPerformance a mm:LivePerformance ;
                             rdfs:comment ?comment .
}

# CQ-L27 (Exit): Did Live Aid inspire later benefit concerts such as Live 8? (RAG-ready)
PREFIX ex: <http://example.org/liveaid/>
PREFIX schema: <https://schema.org/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
SELECT ?event ?eventLabel ?nextEvent ?nextEventLabel ?nextEventURL ?comment
WHERE {
  ex:LiveAid1985 rdfs:label ?eventLabel ;
                 schema:influenced ?nextEvent ;
                 rdfs:comment ?comment .
  BIND(ex:LiveAid1985 AS ?event)
  ?nextEvent rdfs:label ?nextEventLabel .
  OPTIONAL { ?nextEvent schema:sameAs ?nextEventURL }
  OPTIONAL { ?nextEvent rdfs:seeAlso ?nextEventURL }
  OPTIONAL { ?nextEvent schema:subjectOf ?nextEventURL }
}
ORDER BY ?nextEventLabel


# CQ-L28 (Exit): Which artifacts from Live Aid performances are preserved at the BME? (RAG-ready)
PREFIX ex: <http://example.org/liveaid/>
PREFIX schema: <https://schema.org/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
SELECT ?artifact ?artifactLabel ?artifactURL
       ?artist ?artistLabel ?artistURL
       ?location ?locationLabel ?comment
WHERE {
  ?artifact schema:creator ?artist ;
            schema:location ?location ;
            rdfs:comment ?comment ;
            rdfs:label ?artifactLabel .
  ?artist rdfs:label ?artistLabel .
  ?location rdfs:label ?locationLabel .
  OPTIONAL { ?artifact schema:sameAs ?artifactURL }
  OPTIONAL { ?artifact rdfs:seeAlso ?artifactURL }
  OPTIONAL { ?artifact schema:subjectOf ?artifactURL }
  OPTIONAL { ?artist schema:sameAs ?artistURL }
  OPTIONAL { ?artist rdfs:seeAlso ?artistURL }
  OPTIONAL { ?artist schema:subjectOf ?artistURL }
}
ORDER BY ?artifactLabel

