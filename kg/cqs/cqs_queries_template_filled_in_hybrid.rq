#CQ-ID:CQ-H-AF1 What are some notable facts about the music artist {artist}?
PREFIX ex: <http://wembrewind.live/ex#>
PREFIX schema: <http://schema.org/>
SELECT DISTINCT ?artist ?songfactsUrl
WHERE {
  BIND (ex:Madonna AS ?artist)
  ?artist a ?type ;
          schema:sameAs ?songfactsUrl .
  VALUES ?type { schema:MusicGroup schema:Person }
  FILTER( STRSTARTS(LCASE(STR(?songfactsUrl)), "https://www.songfacts.com/facts") )
}


