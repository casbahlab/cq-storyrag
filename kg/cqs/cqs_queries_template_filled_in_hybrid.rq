}


#CQ-ID:CQ-E3 Which [Artist] performed at [Event] held at [Venue]?
PREFIX ex: <http://wembrewind.live/ex#>
PREFIX schema: <http://schema.org/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX oa: <http://www.w3.org/ns/oa#>
PREFIX dct: <http://purl.org/dc/terms/>
SELECT ?sourceUrl ?exactSelector ?refinedBy
WHERE {
    BIND (ex:LiveAid1985 AS ?event)
    BIND (ex:WembleyStadium AS ?venue)
    ?annotation a oa:Annotation ;
           dct:subject ex:Performances ;
           schema:spatial ?venue ;
           oa:hasTarget ?event ;
           oa:hasBody ?background .
    ?background oa:hasSource ?sourceUrl ;
                oa:hasSelector ?secSel .
    OPTIONAL {
        ?secSel oa:refinedBy ?subSel .
        ?secSel oa:exact ?exactSelector .
        OPTIONAL { ?subSel oa:exact ?refinedBy }
        OPTIONAL { ?subSel oa:start ?start ; oa:end ?end }
      }
    ?event schema:name ?eventName .
}


#CQ-ID:CQ-E4 Which [Artist] performed during [Event]?
PREFIX ex: <http://wembrewind.live/ex#>
PREFIX schema: <http://schema.org/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX oa: <http://www.w3.org/ns/oa#>
PREFIX dct: <http://purl.org/dc/terms/>
SELECT ?eventName ?url
WHERE {
    BIND (ex:LiveAid1985 AS ?event)
    ?annotation a oa:Annotation ;
           dct:subject ex:Performances ;
           oa:hasTarget ?event ;
           oa:hasBody ?background .
    ?background schema:citation ?url .
    ?event schema:name ?eventName .
}

#CQ-ID:CQ-E5 Which [City] or [Country] hosted [Event]?
PREFIX ex: <http://wembrewind.live/ex#>
PREFIX schema: <http://schema.org/>
SELECT DISTINCT ?eventName ?locationName ?cityName ?countryName
WHERE {
    ?event a schema:MusicEvent ;
           schema:location ?location .
    ?event schema:name ?eventName .
    OPTIONAL { ?location schema:location ?city .
        	   ?location schema:name ?locationName .
    			}
    OPTIONAL { ?city schema:containedInPlace ?country .
        		?city schema:name ?cityName .
        		?country schema:name ?countryName .
    			}
    FILTER(?event =   ex:LiveAid1985)
}

#CQ-ID:CQ-E6 Which [Venue] hosted [Event]?
PREFIX ex: <http://wembrewind.live/ex#>
PREFIX schema: <http://schema.org/>
SELECT DISTINCT ?eventName (GROUP_CONCAT(DISTINCT ?venueName; separator=", ") AS ?allVenues)
WHERE {
    ?event a schema:MusicEvent ;
           schema:location ?venue .
    ?venue a schema:Place ;
           schema:name ?venueName .
    ?event schema:name ?eventName .
    FILTER(?event =   ex:LiveAid1985)
}
GROUP BY ?eventName


#CQ-ID:CQ-E7 What role did television and media coverage play in [Event]?
PREFIX ex: <http://wembrewind.live/ex#>
PREFIX schema: <http://schema.org/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
