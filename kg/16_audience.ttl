@prefix ex: <http://wembrewind.live/ex#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:GlobalAudience a schema:Audience ;
    schema:audienceType "Worldwide Viewers" ;
    schema:name "Live Aid 1985 Global Audience" ;
    ex:countryCount 150 ;
    ex:viewerCount 1900000000 .

ex:LiveAidAudience a schema:Audience .

ex:LiveAidAudience1985 a schema:Audience ;
    schema:audienceType "Live concert audience" ;
    schema:description "Over 72,000 people attended at Wembley Stadium" ;
    schema:location ex:WembleyStadium ;
    ex:participatedIn ex:ChorusSingalong .

